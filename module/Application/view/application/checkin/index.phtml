
<!--Each view is defined by a data-role="page" attribute-->
<div data-role="page" id="mapPage"
	style="width: 100%; margin-top: 75px; height: 100%; overflow: hidden;"
	data-theme="b">
	<div id="header" data-role="header">
		<h1>Check-in Page</h1>

		<a href="#featureDialog" data-role="button" class="ui-btn-right">Check-in!</a>
	</div>
	<div data-role="content" id="mapcontent"
		style="padding: 0; width: 100%; height: 100%;">
		<div id="map"></div>
	</div>
</div>
<!--Attribute Insepctor Dialog-->
<div data-role="page" id="attributeDialog" data-theme="b">
	<div data-role="header" data-nobackbtn="true">
		<h1>Collect</h1>
	</div>
	<div data-role="content" data-inset="true">
		<div>
			<div id="currentAddress"
				style="font-weight: bold; text-align: center; width: 100%;"></div>
		</div>
		<div id="attributeDiv"></div>
		<a href="#mapPage" data-role="button">Finish</a>
	</div>
</div>
<!--Feature Collection Dialog-->
<div data-role="page" id="featureDialog" data-theme="b">
	<div data-role="header">
		<h1>Feature Type</h1>
	</div>
	<div data-role="content">
		<ul data-inset="true" data-role="listview" id="featureList"></ul>
	</div>
</div>

<!--Checkin Forms Dialog-->
<div data-role="page" id="streamsDialog" data-theme="b">
